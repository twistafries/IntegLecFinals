<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="300.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hangman.QuitController">
    <stylesheets>
        <URL value="@quit.css" />
    </stylesheets>
   <children>
      <Label alignment="CENTER" layoutY="27.0" text="Are you sure you want to quit?" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" layoutY="84.0" spacing="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="yesButton" mnemonicParsing="false" onAction="#yesAction" prefWidth="65.0" text="Yes" />
            <Button fx:id="noButton" mnemonicParsing="false" onAction="#noAction" prefWidth="65.0" text="No" />
         </children>
      </HBox>
   </children>
</AnchorPane>
